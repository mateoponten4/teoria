import numpy as np
import pandas as pd
a = np.array([[1, 2],[ 3, 4],[ 5, 6]])
df = pd.DataFrame(a, index=['fila 1', 'fila 2', 'fila 3'], columns=['columna 1', 'columna 2'])
provincia = ['cordoba','buenos aires', 'santa fe','mendoza','tucuman']
poblado = [40000, 70000, 60000, 80000, 30000]
diccionario = {'provincias: ':provincia, 'poblacion:':poblado}
df2 = pd.DataFrame(diccionario)
print(df2)
